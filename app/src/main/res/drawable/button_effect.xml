<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

<item android:state_pressed="true" >
    <shape>
        <solid
            android:color="@color/forbackgroundbuttonend" />
        <stroke
            android:width="1dp"
            android:color="@color/buttonbordercolorend" />
        <corners
            android:radius="15dp" />


    </shape>
</item>
<item>
    <shape>
        <solid
            android:color="@color/forbackgroundbutton" />
        <stroke
            android:width="1dp"
            android:color="@color/buttonbordercolor" />
        <corners
            android:radius="15dp" />


    </shape>
</item>

</selector>



